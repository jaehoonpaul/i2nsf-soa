.btn-ok:disabled,.btn-ok[disabled]  {opacity: 0.7; cursor: not-allowed;}
.btn-pop-ok { margin: 10px 20px 0px 0px; border: 1px solid #1b9395; background: linear-gradient(to bottom, #24b6b9, #23a5a6); border-radius: 3px; color: #fff; padding: 10px 20px; font-size: 13px; font-weight: bold; width: 97px;}
.btn-pop-ok:active {background: linear-gradient(to top, #24b6b9, #23a5a6);}
.btn-pop-cancel { margin: 10px 20px 0px 0px; border: 1px solid #9a9a9a; background: linear-gradient(to bottom, #cbcbcb, #a6a6a6); border-radius: 3px; color: #fff; padding: 10px 20px; font-size: 13px; font-weight: bold; width: 97px;}
.btn-pop-cancel:active {background: linear-gradient(to top, #cbcbcb, #a6a6a6);}
.btnAdd {margin: 0px 10px 0px 0px; border: 1px solid #1b9395; background: linear-gradient(to bottom, #24b6b9, #23a5a6); border-radius: 3px; color: #fff; padding: 3px 10px; font-size: 13px; font-weight: bold; float:right;}
.btnAdd:active {background: linear-gradient(to top, #24b6b9, #23a5a6);}
.btnRemove {margin: 0px 10px 0px 0px; border: 1px solid #511616; background: linear-gradient(to bottom, #d84545, #a82a2a); border-radius: 3px; color: #fff; padding: 3px 10px; font-size: 13px; font-weight: bold; float:right;}
.btnRemove:active {background: linear-gradient(to top, #d84545, #a82a2a);}

/* jskang 20181123*/
.status-circle-ok {background: linear-gradient(to bottom, #00d873, #008446); height: 80px; width: 80px; border: 3px solid #007a28; border-radius: 40px; margin: auto; color: #000;}
.status-circle-error {background: linear-gradient(to bottom, #ff7f00, #c96400); height: 80px; width: 80px; border: 3px solid #a34c00; border-radius: 40px; middle; margin: auto; color: #000;}
.host-item {float:left; width: 140px; height: 140px; word-break:break-all; margin: 20px 0 20px 20px; font-weight: bold; text-align: center; display: table-cell;}
.vm-item {float:left; width: 140px; height: 140px; word-break:break-all; margin: 20px 0 20px 20px; font-weight: bold; text-align: center;}
.monitoring-host-img {position: absolute; transform: translateY(-50px); width: 140px; text-align: center;}
.monitoring-vm-img {position: absolute; transform: translate(4px, -68px); width: 140px; text-align: center;}
.monitoring-text {background-color: rgba(255, 255, 255, 0.8); border-radius: 5px; padding: 7px 5px 7px 5px; font-size: 13px; border: 1px solid #d6dadd;}
.empty-text {width: 100%; height: 100%; display: table; margin-top: 70px;}
.empty-text span {display: table-cell; vertical-align: middle;}

/* jskang 20190107*/
.select-route {width: 165px;}

/* 공통 css */
.bold {font-weight: bold;}
.nano {width: 50px !important;}
.mini {width: 100px !important;}
.short {width: 200px !important;}
.normal {width: 370px !important;}
.long {width: 542px !important;}
input:read-only {background-color: #eee;}
input:-moz-read-only {background-color: #eee;}
.txt-al {text-align: left !important;}
.txt-ac {text-align: center !important;}
.datepicker.dropdown-menu {z-index:10000 !important;}
#modalContent {width: 800px; height: 700px;}



input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
}
input[type=number] {
    -moz-appearance:textfield;
}

.division { float: left; }