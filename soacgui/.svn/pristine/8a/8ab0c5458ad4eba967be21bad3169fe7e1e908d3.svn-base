/*jskang tooltip 2017-11-06*/
.simplicity_text {
    position: relative;
    display: inline-block;
    /*border-bottom: 1px dotted black;*/
}
.simplicity_text .tooltip_full_text {
    visibility: hidden;
    width: 120px;
    background-color: black;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;

    /* Position the tooltip */
    position: absolute;
    z-index: 1;
    bottom: 150%;
    left: 50%;
    margin-left: -60px;
}


.simplicity_text .tooltip_full_text::after {
    content: "";
    position: absolute;
    top: 100%;
    left: 50%;
    margin-left: -5px;
    border-width: 5px;
    border-style: solid;
    border-color: black transparent transparent transparent;
}

.simplicity_text:hover .tooltip_full_text {
    visibility: visible;
}
